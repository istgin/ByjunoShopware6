{% block cembra_log_entity_list %}
    <sw-page class="byjuno-log-list">
        <template #content>
            {% block cembra_log_entity_list_content %}
                <sw-entity-listing
                    v-if="logs"
                    :items="logs"
                    :repository="repository"
                    :showSelection="false"
                    :columns="columns"
                    :showSettings="false"
                    :showActions="false"
                    detailRoute="byjuno.log.detail">
                </sw-entity-listing>
            {% endblock %}
        </template>
    </sw-page>
{% endblock %}
